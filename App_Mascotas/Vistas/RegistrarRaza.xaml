<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App_Mascotas.Vistas.RegistrarRaza">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNombre" Placeholder="Ingrese Nombre" />
            <Entry x:Name="txtCaracter" Placeholder="Ingrese Caracteristica" />
            <Button x:Name="btnAgregar" Clicked="btnAgregar_Clicked" />
            <Button x:Name="btnEliminar" Text="Eliminar" Clicked="btnEliminar_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>